<mapper namespace="com.mengxuegu.blog.question.mapper.QuestionLabelMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.mengxuegu.blog.entities.QuestionLabel">
        <id column="id" property="id" />
